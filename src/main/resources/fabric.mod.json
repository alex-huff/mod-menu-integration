{
  "schemaVersion": 1,
  "id": "mod-menu-integration",
  "version": "${version}",
  "name": "Mod Menu Integration",
  "description": "Remotely retrieve and open mod configuration pages using Mod Menu",
  "authors": [
    "phonis"
  ],
  "contact": {
    "sources": "https://github.com/alex-huff/mod-menu-integration"
  },
  "license": "CC0-1.0",
  "icon": "assets/mmintegration/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.alexfh.mmintegration.ModMenuIntegration"
    ]
  },
  "mixins": [
    "mmintegration.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.21",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*",
    "modmenu": ">=7.1.0"
  },
  "custom": {
    "modmenu": {
      "links": {
        "github": "https://github.com/alex-huff/mod-menu-integration"
      }
    }
  }
}