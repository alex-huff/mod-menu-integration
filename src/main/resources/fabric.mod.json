{
  "schemaVersion": 1,
  "id": "mc-cli",
  "version": "${version}",
  "name": "mc-cli",
  "description": "Remotely control a Minecraft instance",
  "authors": [
    "phonis"
  ],
  "contact": {
    "sources": "https://github.com/alex-huff/mc-cli"
  },
  "license": "CC0-1.0",
  "icon": "assets/mccli/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.alexfh.mccli.MCCLI"
    ]
  },
  "mixins": [
    "mccli.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.9",
    "minecraft": "~1.21.4",
    "java": ">=21",
    "fabric-api": "*",
    "modmenu": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "github": "https://github.com/alex-huff/mc-cli"
      }
    }
  }
}
